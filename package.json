{
  "name": "tol_server",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc",
    "clean-install": "rm -rf ./node_modules && rm -r package-lock.json && npm i",
    "dev": "cross-env NODE_ENV=development ts-node src/server.ts",
    "dev:hot": "nodemon --exec \"npm run dev\" --watch ./src --ext .ts",
    "lint": "eslint . --ext .ts",
    "start": "NODE_ENV=production node dist/server.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:hot": "nodemon --exec \"npm run test\" --watch ./src --watch ./spec --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^8.0.0",
    "jet-logger": "^2.0.1",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "pg": "^8.13.3",
    "pg-format": "^1.0.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@stylistic/eslint-plugin-js": "^4.1.0",
    "@stylistic/eslint-plugin-ts": "^4.1.0",
    "@swc/core": "^1.11.1",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.0",
    "@types/find": "^0.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/jasmine": "^5.1.7",
    "@types/jest": "^29.5.14",
    "@types/jsonfile": "^6.1.4",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.5",
    "@types/pg": "^8.11.11",
    "@types/pg-format": "^1.0.5",
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.21.0",
    "eslint-plugin-n": "^17.15.1",
    "find": "^0.3.0",
    "fs-extra": "^11.3.0",
    "jasmine": "^5.6.0",
    "jest": "^29.7.0",
    "jiti": "^2.4.2",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0"
  }
}
